{
  "name": "@dc7290/next-router-prefetch",
  "version": "3.0.2",
  "description": "This provides a custom hook that contains 'next/router'.",
  "homepage": "https://github.com/dc7290/next-router-prefetch#next-router-prefetch",
  "repository": {
    "type": "git",
    "url": "https://github.com/dc7290/next-router-prefetch"
  },
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "typings": "lib/index.d.ts",
  "author": "dc7290",
  "license": "MIT",
  "keywords": [
    "react",
    "hooks",
    "next",
    "prefetch",
